install.packages("ggplot2")
library(dplyr)
midwest <- as.data.frame(ggplot2::midwest)
head(midwest)
View(midwest)
dim(midwest)
str(midwest)
summary(midwest)
tail(midwest)
re_midwest <- midwest
re_midwest <- rename(re_midwest, total = poptotal, asian = popasian)
View(re_midwest)
re_midwest$p_percent <- (re_midwest$asian/re_midwest$total)*100
hist(re_midwest$p_percent)
re_midwest$mean <- mean(re_midwest$p_percent) 
re_midwest$size <- ifelse(re_midwest$p_percent > re_midwest$mean, "large", "small")
table(re_midwest$size)
qplot(re_midwest$size)
filter(re_midwest)
select(re_midwest)
